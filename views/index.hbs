{{!-- FORM --}}

<form class="form-discount">
  <h2>Receba o seu desconto e uma muda de tempero na primeira compra</h2>
  <div class="form-group">
    <label for="exampleInputEmail1">Nome</label>
    <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Digite seu nome">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">E-mail</label>
    <input type="e-mail" class="form-control" id="exampleInputPassword1" placeholder="Digite seu e-mail">
  </div>
  <button type="submit" class="btn btn-primary">Quero um cupom de desconto</button>
</form>

{{!-- PRODUCT CARDS --}}

<section>

<div class="container">
  <h3 class='second-title-index' id='card-header'>Encontrar o local perfeito é fácil!</h3>
  <div class="card-deck">
    <div class="card" id='comofunciona' style="border: none;">
      <img src="https://www.saboravida.com.br/wp-content/uploads/2017/09/comida-congelada-e-opcao-para-manter-uma-alimentacao-saudavel.jpg" class="card-img-top card-logo" alt="Escolha o Seu Espaço">
      <div class="card-body">
        <h5 class="card-title">Escolha</h5>
        <p class="card-text">Analise todas as informações e escolha seu espaço preferido.</p>
      </div>
    </div>
    <div class="card" style="border: none;">
      <img src="https://www.saboravida.com.br/wp-content/uploads/2017/09/comida-congelada-e-opcao-para-manter-uma-alimentacao-saudavel.jpg" class="card-img-top card-logo" alt="Solicite um Orçamento para o Evento">
